{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ConnectionStrings": {
    "SQLServer": "__ Must be overridden locally or set from installer. __"
  },
  "ApplySqlMigrations": false,
  "RFID": {
    "AutoRead": true,
    "ReadingSpeed": 2,
    "ReaderType": "NordicIdOrCAEN"
  },
  "UseGroups": false,
  "UILanguage": "en-US",
  "StationUniqueID": "Technical Station",
  "VendorName": "Caretag",
  "NextInternalId": 0,
  "ResetEnabled": false
}